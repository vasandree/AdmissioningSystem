  "components": {
    "schemas": {
      "Response": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        }
      },
      "Passport": {
        "type": "object",
        "required": [
          "id",
          "seriesAndNumber",
          "placeOfBirth",
          "issueDate",
          "issuedBy"
        ],
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "seriesAndNumber": {
            "type": "string"
          },
          "placeOfBirth": {
            "type": "string"
          },
          "issueDate": {
            "type": "string",
            "format": "date-time"
          },
          "issuedBy": {
            "type": "string"
          }
        }
      },
      "EducationDocument": {
        "required": [
          "createTime",
          "educationLevel",
          "id",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "createTime": {
            "type": "string",
            "format": "date-time"
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "educationLevel": {
            "$ref": "#/components/schemas/EducationLevelModel"
          },
          "nextEducationLevels": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/EducationLevelModel"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "EducationLevelModel": {
        "required": [
          "id",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "EditPassport": {
