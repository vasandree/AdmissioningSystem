{
  "openapi": "3.0.3",
  "info": {
    "title": "Admission Service 1.0",
    "description": "",
    "version": "1.1.0"
  },
  "tags": [
    {
      "name": "AdmissionManaging"
    },
    {
      "name": "PriorityList"
    }
  ],
  "paths": {
    "/student/admission": {
      "post": {
        "tags": [
          "AdmissionManaging"
        ],
        "summary": "Creating admission",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AdmissionCreate"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Admission status changed successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "InternalServerError"
          }
        }
      }
    },
    "/student/{AdmissionId}": {
      "delete": {
        "tags": [
          "AdmissionManaging"
        ],
        "summary": "Deleting admission",
        "parameters": [
          {
            "name": "AdmissionId",
            "in": "path",
            "description": "Admission ID",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Admission status changed successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "InternalServerError"
          }
        }
      }
    },
    "/manager/{AdmissionId}": {
      "patch": {
        "tags": [
          "AdmissionManaging"
        ],
        "summary": "Change admission status",
        "parameters": [
          {
            "name": "AdmissionId",
            "in": "path",
            "description": "Admission ID",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": {
                    "$ref": "#/components/schemas/AdmissionStatus"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Admission status changed successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Response"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "InternalServerError"
          }
        }
      }
    },
    "/priorityList": {
      "get": {
        "tags": [
          "PriorityList"
        ],
        "summary": "Getting my admissions in priority list",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Got admissions successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/MyAdmission"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "InternalServerError"
          }
        }
      }
    },
    "/priorityList/{AdmissionId}": {
      "patch": {
        "tags": [
          "PriorityList"
        ],
        "summary": "Change priority of admission",
        "parameters": [
          {
            "name": "AdmissionId",
            "in": "path",
            "description": "Admission ID",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "priority": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Admissions priority successfully changed",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Response"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/manager/{ApplicantId}/priorityList/{AdmissionId}": {
      "patch": {
        "tags": [
          "PriorityList"
        ],
        "summary": "Change priority of admission",
        "parameters": [
          {
            "name": "AdmissionId",
            "in": "path",
            "description": "Admission ID",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ApplicantId",
            "in": "path",
            "description": "Applicant ID",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "priority": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Admissions priority successfully changed",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Response"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Response": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        }
      },
      "AdmissionStatus": {
        "type": "string",
        "enum": [
          "created",
          "underConsideration",
          "confirmed",
          "rejected",
          "closed"
        ]
      },
      "AdmissionCreate": {
        "type": "object",
        "properties": {
          "programmeId": {
            "type": "string",
            "format": "uuid"
          },
          "priority": {
            "type": "integer"
          }
        }
      },
      "MyAdmission": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "priority": {
            "type": "integer"
          },
          "status": {
            "$ref": "#/components/schemas/AdmissionStatus"
          },
          "programme": {
            "$ref": "#/components/schemas/EducationProgramModel"
          }
        }
      },
      "EducationLevelModel": {
        "required": [
          "id",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "minLength": 1
          }
        },
        "additionalProperties": false
      },
      "EducationProgramModel": {
        "required": [
          "createTime",
          "educationForm",
          "educationLevel",
          "faculty",
          "id",
          "language",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "createTime": {
            "type": "string",
            "format": "date-time"
          },
          "name": {
            "type": "string",
            "minLength": 1
          },
          "code": {
            "type": "string",
            "nullable": true
          },
          "language": {
            "type": "string",
            "minLength": 1
          },
          "educationForm": {
            "type": "string",
            "minLength": 1
          },
          "faculty": {
            "$ref": "#/components/schemas/FacultyModel"
          },
          "educationLevel": {
            "$ref": "#/components/schemas/EducationLevelModel"
          }
        },
        "additionalProperties": false
      },
      "FacultyModel": {
        "required": [
          "createTime",
          "id",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "createTime": {
            "type": "string",
            "format": "date-time"
          },
          "name": {
            "type": "string",
            "minLength": 1
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}